{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"max-len": [
			1,
			80
		],
		"space-before-paren": 0,
		"quotes": [
			2,
			"single",
			{
				"avoidEscape": true
			}
		]
	}
}